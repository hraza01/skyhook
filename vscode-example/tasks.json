{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Deploy DAG to Cloud Composer",
            "type": "shell",
            "command": "skyhook ${workspaceFolder}",
            "problemMatcher": [],
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": true,
                "panel": "dedicated",
                "showReuseMessage": true,
                "clear": true
            },
            "options": {
                "env": {
                    "GCS_BUCKET_NAME": "<your-bucket-name>",
                    "COMPOSER_URL_BASE": "<your-composer-url-base>"
                }
            }
        }
    ]
}
